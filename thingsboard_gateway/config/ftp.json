        },
        {

          "type": "string",
          "key": "30_Day_Avail_%",
          "ts" : "${LastUpdate}",
          "value": "${30_Day_Avail_Percentage}"
        }
      ]
    }
  ],
  "serverSideRpc": [
    {
      "deviceNameFilter": ".*",
      "methodFilter": "read",
      "valueExpression": "${params}"
    },
    {
      "deviceNameFilter": ".*",
